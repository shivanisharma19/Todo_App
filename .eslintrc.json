{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
        
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "import/resolver" : {  // for not getting error on unused import or function
            "node" : {
                "extensions" : [".js"," .jsx"] 
            }
        },
        "react" : {
            "version" : "detect"
        }
    },
    "rules": {"react/prop-types": 0,
    "no-unused-vars": "off"
    }
}
